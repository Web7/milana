mixin FormStyleContainerItem(data)

	.mt-13.mx-13
		p.f-extrabold.f-33px.form-style-title.text-uppercase.line-height-1.mb-7= data.text1
		.d-flex.flex-row
			if data.buttonsCount === 2
				.d-flex
					a.btn.btn-primary.f-medium.f-15px(href="#") Логотип в pdf
					a.btn.btn-primary.form-style-btn.f-medium.f-15px(href="#") Логотип в кривых
			if data.buttonsCount === 3
				.d-flex
					a.btn.btn-primary.f-medium.f-15px(href="#") Гайдлайн в pdf
					a.btn.btn-primary.form-style-btn.f-medium.f-15px(href="#") Гайдлайн в кривых